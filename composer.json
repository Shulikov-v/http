{
    "name": "icicleio/http",
    "description": "Asynchronous HTTP component for Icicle.",
    "keywords": [
        "http",
        "asynchronous",
        "async"
    ],
    "homepage": "http://icicle.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@icicle.io"
        }
    ],
    "require": {
        "icicleio/icicle": "~0.4",
        "icicleio/dns": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "mockery/mockery": "0.9.*",
        "satooshi/php-coveralls": "dev-master",
        "symfony/yaml": "~2"
    },
    "autoload": {
        "psr-4": {
            "Icicle\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icicle\\Tests\\": "vendor/icicleio/icicle/tests",
            "Icicle\\Tests\\Http\\": "tests"
        }
    }
}
